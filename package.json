{
  "name": "ci_analyzer",
  "version": "6.0.0",
  "description": "Export multiple CI service build data for analyzing.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kesin11/CIAnalyzer.git"
  },
  "keywords": [
    "CI"
  ],
  "author": "kesin1202000@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kesin11/CIAnalyzer/issues"
  },
  "homepage": "https://github.com/Kesin11/CIAnalyzer#readme",
  "engines": {
    "node": ">=20.10.0"
  },
  "bin": {
    "ci_analyzer": "dist/index.js"
  },
  "scripts": {
    "start": "node --enable-source-maps dist/index.js",
    "clean": "rm -rf dist",
    "build": "esbuild --bundle --format=cjs --platform=node --target=node20 --sourcemap --outdir=dist src/index.ts",
    "build:clean": "npm run clean && npm run build",
    "check": "tsc --noEmit",
    "check:watch": "tsc --watch --noEmit",
    "lint": "echo \"We have not lint yet\"",
    "test": "jest",
    "test:ci": "jest --coverage --ci --runInBand",
    "test:watch": "jest --watch",
    "proto": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +proto",
    "docker": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +docker",
    "schema": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +schema"
  },
  "dependencies": {
    "@google-cloud/bigquery": "7.3.0",
    "@google-cloud/storage": "7.7.0",
    "@octokit/plugin-retry": "6.0.1",
    "@octokit/plugin-throttling": "8.1.3",
    "@octokit/rest": "20.0.2",
    "adm-zip": "0.5.10",
    "axios": "1.6.3",
    "axios-retry": "4.0.0",
    "dayjs": "1.11.10",
    "js-yaml": "4.1.0",
    "junit2json": "3.1.5",
    "lodash": "4.17.21",
    "minimatch": "9.0.3",
    "tslog": "4.9.2",
    "utility-types": "3.10.0",
    "yargs": "17.7.2",
    "zod": "3.22.4",
    "zod-to-json-schema": "3.22.3"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.5",
    "@types/jest": "29.5.11",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.6",
    "@types/yargs": "17.0.32",
    "esbuild": "0.19.10",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "ts-jest": "29.1.1",
    "ts-proto": "1.165.3",
    "ts-protoc-gen": "0.15.0",
    "typescript": "5.3.3"
  }
}
