{
  "name": "ci_analyzer",
  "version": "5.2.0",
  "description": "Export multiple CI service build data for analyzing.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kesin11/CIAnalyzer.git"
  },
  "keywords": [
    "CI"
  ],
  "author": "kesin1202000@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kesin11/CIAnalyzer/issues"
  },
  "homepage": "https://github.com/Kesin11/CIAnalyzer#readme",
  "engines": {
    "node": ">=v18.17.1"
  },
  "bin": {
    "ci_analyzer": "dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "build": "tsc -b",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "tsc -b -w",
    "lint": "echo \"We have not lint yet\"",
    "test": "jest",
    "test:ci": "jest --coverage --ci --runInBand",
    "test:watch": "jest --watch",
    "proto": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +proto",
    "docker": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +docker",
    "schema": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +schema"
  },
  "dependencies": {
    "@google-cloud/bigquery": "7.2.0",
    "@google-cloud/storage": "6.12.0",
    "@octokit/plugin-retry": "6.0.0",
    "@octokit/plugin-throttling": "7.0.0",
    "@octokit/rest": "20.0.1",
    "adm-zip": "0.5.10",
    "axios": "1.4.0",
    "axios-concurrency": "1.0.4",
    "axios-retry": "3.6.0",
    "dayjs": "1.11.9",
    "js-yaml": "4.1.0",
    "junit2json": "3.1.2",
    "lodash": "4.17.21",
    "minimatch": "9.0.3",
    "tslog": "4.9.1",
    "utility-types": "3.10.0",
    "yargs": "17.7.2",
    "zod": "3.22.2",
    "zod-to-json-schema": "3.21.4"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.0",
    "@types/jest": "29.5.3",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "4.14.197",
    "@types/node": "18.17.7",
    "@types/yargs": "17.0.24",
    "jest": "29.6.2",
    "jest-junit": "16.0.0",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.1",
    "ts-proto": "1.156.6",
    "ts-protoc-gen": "0.15.0",
    "typescript": "5.1.6"
  }
}
