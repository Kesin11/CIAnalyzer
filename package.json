{
  "name": "ci_analyzer",
  "version": "4.4.0",
  "description": "Export multiple CI service build data for analyzing.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kesin11/CIAnalyzer.git"
  },
  "keywords": [
    "CI"
  ],
  "author": "kesin1202000@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kesin11/CIAnalyzer/issues"
  },
  "homepage": "https://github.com/Kesin11/CIAnalyzer#readme",
  "bin": {
    "ci_analyzer": "dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "build": "tsc -b",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "tsc -b -w",
    "lint": "echo \"We have not lint yet\"",
    "test": "jest",
    "test:ci": "jest --coverage --ci --runInBand",
    "test:watch": "jest --watch",
    "coverage": "codecov",
    "release:prepare": "npx shipjs prepare",
    "release:trigger": "npx shipjs trigger",
    "proto": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +proto",
    "docker": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +docker"
  },
  "dependencies": {
    "@google-cloud/bigquery": "5.10.0",
    "@google-cloud/storage": "5.17.0",
    "@octokit/plugin-retry": "3.0.9",
    "@octokit/plugin-throttling": "3.5.2",
    "@octokit/rest": "18.12.0",
    "@types/minimatch": "3.0.5",
    "adm-zip": "0.5.9",
    "axios": "0.25.0",
    "axios-retry": "3.2.4",
    "dayjs": "1.10.7",
    "js-yaml": "4.1.0",
    "junit2json": "2.0.5",
    "lodash": "4.17.21",
    "minimatch": "3.0.4",
    "tslog": "3.3.1",
    "utility-types": "3.10.0",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "@types/adm-zip": "0.4.34",
    "@types/jest": "27.4.0",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.21",
    "@types/yargs": "17.0.8",
    "codecov": "3.8.3",
    "jest": "27.4.7",
    "jest-junit": "13.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "ts-proto": "1.100.1",
    "ts-protoc-gen": "0.15.0",
    "typescript": "4.5.4"
  }
}
