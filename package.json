{
  "name": "ci_analyzer",
  "version": "5.1.1",
  "description": "Export multiple CI service build data for analyzing.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kesin11/CIAnalyzer.git"
  },
  "keywords": [
    "CI"
  ],
  "author": "kesin1202000@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kesin11/CIAnalyzer/issues"
  },
  "homepage": "https://github.com/Kesin11/CIAnalyzer#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "ci_analyzer": "dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "build": "tsc -b",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "tsc -b -w",
    "lint": "echo \"We have not lint yet\"",
    "test": "jest",
    "test:ci": "jest --coverage --ci --runInBand",
    "test:watch": "jest --watch",
    "coverage": "codecov",
    "proto": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +proto",
    "docker": "earthly --strict --remote-cache=ghcr.io/kesin11/ci_analyzer_earthly:cache +docker"
  },
  "dependencies": {
    "@google-cloud/bigquery": "6.0.3",
    "@google-cloud/storage": "6.7.0",
    "@octokit/plugin-retry": "4.0.3",
    "@octokit/plugin-throttling": "4.3.2",
    "@octokit/rest": "19.0.5",
    "@types/minimatch": "5.1.2",
    "adm-zip": "0.5.9",
    "axios": "0.27.2",
    "axios-concurrency": "1.0.4",
    "axios-retry": "3.3.1",
    "dayjs": "1.11.6",
    "js-yaml": "4.1.0",
    "junit2json": "3.0.3",
    "lodash": "4.17.21",
    "minimatch": "5.1.0",
    "tslog": "3.3.4",
    "utility-types": "3.10.0",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.0",
    "@types/jest": "29.2.1",
    "@types/js-yaml": "4.0.5",
    "@types/lodash": "4.14.188",
    "@types/node": "18.11.9",
    "@types/yargs": "17.0.13",
    "codecov": "3.8.3",
    "jest": "29.2.2",
    "jest-junit": "14.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-proto": "1.131.0",
    "ts-protoc-gen": "0.15.0",
    "typescript": "4.8.4"
  }
}
